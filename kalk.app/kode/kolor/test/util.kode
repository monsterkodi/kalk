use ../kolor

ext = 'kode'

export
    lang: (l) -> ext = l
    ranges: (s) -> kolor.ranges s, ext
    dissect: (c) -> kolor.dissect c.split('\n'), ext
    
    inc: (rgs, start, match) -> 
        for r in rgs ➜ if r.start == start and r.match == match ➜ ⮐ r.clss
        log 'fail' rgs